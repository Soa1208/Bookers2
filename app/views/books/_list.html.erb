<h1>User Info</h1>
<%= image_tag @user.get_profile_image %>
<p>name：<%= @user.name %></p>
<p>introduction：<%= @user.introduction %></p>
<% if @user == current_user %>
  <p><%= link_to "Edit", edit_user_path(@user) %></p>
<% else %>
  <p><%= link_to "Go current_user", user_path(current_user) %></p>
<% end %>

新規投稿フォーム
<%= form_with model: book do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
	
  <h4>本文</h4>
  <%= f.text_area :body %>
	
  <%= f.submit '投稿' %>
<% end %>